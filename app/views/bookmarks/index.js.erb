$('.bl_bookmarks__list').append('<%= j render(@bookmarks) %>');
<% if @bookmarks.next_page %>
  // $('.digg_pagination').replaceWith('<%= j will_paginate(@bookmarks) %>');
  $('.digg_pagination').replaceWith('<%= j render "paginate", object: @bookmarks %>');
<% else %>
  $('.digg_pagination').remove();
<% end %>
